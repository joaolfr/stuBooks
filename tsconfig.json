{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "typeRoots": ["./src/@types"],
    "paths":{
      "@assets/*":[
        "./src/assets/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@components/*":[
        "./src/components/*"
      ],
      "@routes/*":[
        "./src/routes/*"
      ],
      "@storage/*":[
        "./src/storage/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],
      "@theme/*":[
        "./src/theme/*"
      ],
       "@api/*":[
        "./src/api/*"
      ],
      "@hooks/*":[
        "./src/hooks/*"
      ],
      "@events/*":[
        "./src/utils/events/*"
      ],
      
    }
  }
}
